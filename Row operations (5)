BLUE=document.getElementById("bluetip")
  GREEN=document.getElementById("greentip")
  RED=document.getElementById("redtip")
  YELLOW=document.getElementById("yellowtip")

  BLUE.style.backgroundColor="blue"
  GREEN.style.backgroundColor="green"
  RED.style.backgroundColor="red"
  YELLOW.style.backgroundColor="yellow"

function set(complexity){
B0=Math.round(10*(Math.random()+Math.random()))-10
B1=Math.round(10*(Math.random()+Math.random()))-10
B2=Math.round(10*(Math.random()+Math.random()))-10
B3=Math.round(10*(Math.random()+Math.random()))-10
b=[[1,1],[0,1],[0,1],[0,1],[B0,1]]
g=[[0,1],[1,1],[0,1],[0,1],[B1,1]]
r=[[0,1],[0,1],[1,1],[0,1],[B2,1]]
y=[[0,1],[0,1],[0,1],[1,1],[B3,1]]
var Colors=[b,g,r,y]
for (j=0; j<complexity; j++){
    if (Math.random()<0.5){
        m=Math.floor(4*Math.random())
        n=Math.floor(4*Math.random())
        var Cm=Colors[m]
        var Cn=Colors[n]
        Colors[m]=Cn
        Colors[n]=Cm}


    if (Math.random()<0.5){
        m=Math.floor(4*Math.random())
        c=Math.round(10*Math.random())-5
            for (k=0; k<5; k++){
                Colors[m][k][0]*=c}}

      if (Math.random()<0.3){
        m=Math.floor(4*Math.random())
        c=Math.ceil(4*Math.random())
            for (k=0; k<5; k++){
                Colors[m][k][1]*=c}}


    if (Math.random()<0.9){
        m=Math.floor(4*Math.random())
        n=Math.floor(4*Math.random())
        for (k=0; k<5; k++){
                var t0=Colors[m][k][0]*Colors[n][k][1]
                    +Colors[m][k][1]*Colors[n][k][0]
                var t1=Colors[m][k][1]*Colors[n][k][1]
                Colors[n][k][0]=t0
                Colors[n][k][1]=t1
            }}
       

}
b1=document.getElementById("blue1")
    b2=document.getElementById("blue2")
    b3=document.getElementById("blue3")
    b4=document.getElementById("blue4")
    b5=document.getElementById("blue5")
    g1=document.getElementById("green1")
    g2=document.getElementById("green2")
    g3=document.getElementById("green3")
    g4=document.getElementById("green4")
    g5=document.getElementById("green5")
    r1=document.getElementById("red1")
    r2=document.getElementById("red2")
    r3=document.getElementById("red3")
    r4=document.getElementById("red4")
    r5=document.getElementById("red5")
    y1=document.getElementById("yellow1")
    y2=document.getElementById("yellow2")
    y3=document.getElementById("yellow3")
    y4=document.getElementById("yellow4")
    y5=document.getElementById("yellow5")  
    
     B=[b1,b2,b3,b4,b5]
     G=[g1,g2,g3,g4,g5]
     R=[r1,r2,r3,r4,r5]
    Y=[y1,y2,y3,y4,y5]

    var COLORS=[B,G,R,Y]
    for (p=0; p<4; p++){
      for (q=0; q<5; q++){
   



        if (Colors[p][q][1]==1 || Colors[p][q][0]==0){
          COLORS[p][q].innerHTML=Colors[p][q][0]
        }
        
        if (Colors[p][q][1]!=1 && Colors[p][q][0]!=0){
          COLORS[p][q].innerHTML=Colors[p][q][0]+'&frasl;'+Colors[p][q][1]
        }


  
    }
  }

}




function write_equations(){
  b1=document.getElementById("blue1").innerHTML
    b2=document.getElementById("blue2").innerHTML
    b3=document.getElementById("blue3").innerHTML
    b4=document.getElementById("blue4").innerHTML
    b5=document.getElementById("blue5").innerHTML
    g1=document.getElementById("green1").innerHTML
    g2=document.getElementById("green2").innerHTML
    g3=document.getElementById("green3").innerHTML
    g4=document.getElementById("green4").innerHTML
    g5=document.getElementById("green5").innerHTML
    r1=document.getElementById("red1").innerHTML
    r2=document.getElementById("red2").innerHTML
    r3=document.getElementById("red3").innerHTML
    r4=document.getElementById("red4").innerHTML
    r5=document.getElementById("red5").innerHTML
    y1=document.getElementById("yellow1").innerHTML
    y2=document.getElementById("yellow2").innerHTML
    y3=document.getElementById("yellow3").innerHTML
    y4=document.getElementById("yellow4").innerHTML
    y5=document.getElementById("yellow5").innerHTML
    B=document.getElementById("bluetip").style.backgroundColor
    G=document.getElementById("greentip").style.backgroundColor
    R=document.getElementById("redtip").style.backgroundColor
    Y=document.getElementById("yellowtip").style.backgroundColor

    Eq=document.getElementById("equations")
    Eq.innerHTML=b1+"x+"+b2+"y+"+b3+"z+"+b4+"w="+b5 
 


}


set(6)
reduce()
write_equations()
States=[]

 



function record_state(){
  b1=document.getElementById("blue1").innerHTML
    b2=document.getElementById("blue2").innerHTML
    b3=document.getElementById("blue3").innerHTML
    b4=document.getElementById("blue4").innerHTML
    b5=document.getElementById("blue5").innerHTML
    g1=document.getElementById("green1").innerHTML
    g2=document.getElementById("green2").innerHTML
    g3=document.getElementById("green3").innerHTML
    g4=document.getElementById("green4").innerHTML
    g5=document.getElementById("green5").innerHTML
    r1=document.getElementById("red1").innerHTML
    r2=document.getElementById("red2").innerHTML
    r3=document.getElementById("red3").innerHTML
    r4=document.getElementById("red4").innerHTML
    r5=document.getElementById("red5").innerHTML
    y1=document.getElementById("yellow1").innerHTML
    y2=document.getElementById("yellow2").innerHTML
    y3=document.getElementById("yellow3").innerHTML
    y4=document.getElementById("yellow4").innerHTML
    y5=document.getElementById("yellow5").innerHTML
    B=document.getElementById("bluetip").style.backgroundColor
    G=document.getElementById("greentip").style.backgroundColor
    R=document.getElementById("redtip").style.backgroundColor
    Y=document.getElementById("yellowtip").style.backgroundColor


    States.push([[b1,b2,b3,b4,b5],[g1,g2,g3,g4,g5],[r1,r2,r3,r4,r5],[y1,y2,y3,y4,y5],[B,G,R,Y]])
}

function revert(){
  S=States[States.length-1]
  document.getElementById("blue1").innerHTML=S[0][0]
    document.getElementById("blue2").innerHTML=S[0][1]
   document.getElementById("blue3").innerHTML=S[0][2]
    document.getElementById("blue4").innerHTML=S[0][3]
    document.getElementById("blue5").innerHTML=S[0][4]
    document.getElementById("green1").innerHTML=S[1][0]
    document.getElementById("green2").innerHTML=S[1][1]
    document.getElementById("green3").innerHTML=S[1][2]
    document.getElementById("green4").innerHTML=S[1][3]
    document.getElementById("green5").innerHTML=S[1][4]
    document.getElementById("red1").innerHTML=S[2][0]
    document.getElementById("red2").innerHTML=S[2][1]
    document.getElementById("red3").innerHTML=S[2][2]
    document.getElementById("red4").innerHTML=S[2][3]
    document.getElementById("red5").innerHTML=S[2][4]
    document.getElementById("yellow1").innerHTML=S[3][0]
    document.getElementById("yellow2").innerHTML=S[3][1]
    document.getElementById("yellow3").innerHTML=S[3][2]
    document.getElementById("yellow4").innerHTML=S[3][3]
    document.getElementById("yellow5").innerHTML=S[3][4]
    document.getElementById("bluetip").style.backgroundColor=S[4][0]
    document.getElementById("blue1").style.backgroundColor=S[4][0]
    document.getElementById("blue2").style.backgroundColor=S[4][0]
    document.getElementById("blue3").style.backgroundColor=S[4][0]
    document.getElementById("blue4").style.backgroundColor=S[4][0]
    document.getElementById("blue5").style.backgroundColor=S[4][0]

    document.getElementById("green1").style.backgroundColor=S[4][1]
    document.getElementById("green2").style.backgroundColor=S[4][1]
    document.getElementById("green3").style.backgroundColor=S[4][1]
    document.getElementById("green4").style.backgroundColor=S[4][1]
    document.getElementById("green5").style.backgroundColor=S[4][1]
    document.getElementById("greentip").style.backgroundColor=S[4][1]


    document.getElementById("redtip").style.backgroundColor=S[4][2]
    document.getElementById("red1").style.backgroundColor=S[4][2]
    document.getElementById("red2").style.backgroundColor=S[4][2]
    document.getElementById("red3").style.backgroundColor=S[4][2]
    document.getElementById("red4").style.backgroundColor=S[4][2]
    document.getElementById("red5").style.backgroundColor=S[4][2]

    document.getElementById("yellowtip").style.backgroundColor=S[4][3]
    document.getElementById("yellow1").style.backgroundColor=S[4][3]
    document.getElementById("yellow2").style.backgroundColor=S[4][3]
    document.getElementById("yellow3").style.backgroundColor=S[4][3]
    document.getElementById("yellow4").style.backgroundColor=S[4][3]
    document.getElementById("yellow5").style.backgroundColor=S[4][3]

  States.splice(States.length-1,1)
}





function swi(i){
  record_state()
  BLUE=document.getElementById("bluetip")
  GREEN=document.getElementById("greentip")
  RED=document.getElementById("redtip")
  YELLOW=document.getElementById("yellowtip")
    b1=document.getElementById("blue1")
    b2=document.getElementById("blue2")
    b3=document.getElementById("blue3")
    b4=document.getElementById("blue4")
    b5=document.getElementById("blue5")
    g1=document.getElementById("green1")
    g2=document.getElementById("green2")
    g3=document.getElementById("green3")
    g4=document.getElementById("green4")
    g5=document.getElementById("green5")
    r1=document.getElementById("red1")
    r2=document.getElementById("red2")
    r3=document.getElementById("red3")
    r4=document.getElementById("red4")
    r5=document.getElementById("red5")
    y1=document.getElementById("yellow1")
    y2=document.getElementById("yellow2")
    y3=document.getElementById("yellow3")
    y4=document.getElementById("yellow4")
    y5=document.getElementById("yellow5")  
    
    B=[b1,b2,b3,b4,b5]
    G=[g1,g2,g3,g4,g5]
    R=[r1,r2,r3,r4,r5]
    Y=[y1,y2,y3,y4,y5]
    Colors=[B,G,R,Y]
    COL=[BLUE,GREEN,RED,YELLOW]
    First=Colors[i-1]
    Second=Colors[i]
    FirstColor=COL[i-1].style.backgroundColor
    SecondColor=COL[i].style.backgroundColor
    var FirstHTML=[]
    var SecondHTML=[]
    for (var j=0; j<5; j++){
    FirstHTML.push(First[j].innerHTML)
    SecondHTML.push(Second[j].innerHTML)
    }

    for (var j=0; j<5; j++){
    First[j].innerHTML=SecondHTML[j]
    First[j].style.backgroundColor=SecondColor
    Second[j].innerHTML=FirstHTML[j]
    Second[j].style.backgroundColor=FirstColor
    }

    COL[i-1].style.backgroundColor=SecondColor
    COL[i].style.backgroundColor=FirstColor



    }





    function mult(i){
      record_state()
    b1=document.getElementById("blue1")
    b2=document.getElementById("blue2")
    b3=document.getElementById("blue3")
    b4=document.getElementById("blue4")
    b5=document.getElementById("blue5")
    g1=document.getElementById("green1")
    g2=document.getElementById("green2")
    g3=document.getElementById("green3")
    g4=document.getElementById("green4")
    g5=document.getElementById("green5")
    r1=document.getElementById("red1")
    r2=document.getElementById("red2")
    r3=document.getElementById("red3")
    r4=document.getElementById("red4")
    r5=document.getElementById("red5")
    y1=document.getElementById("yellow1")
    y2=document.getElementById("yellow2")
    y3=document.getElementById("yellow3")
    y4=document.getElementById("yellow4")
    y5=document.getElementById("yellow5")  

    b=document.getElementById("in1")
    g=document.getElementById("in2")
    r=document.getElementById("in3")
    y=document.getElementById("in4")

    constants=[b,g,r,y]
    
    B=[b1,b2,b3,b4,b5]
    G=[g1,g2,g3,g4,g5]
    R=[r1,r2,r3,r4,r5]
    Y=[y1,y2,y3,y4,y5]
    Colors=[B,G,R,Y] 
    Row=Colors[i-1]
    Con=constants[i-1].value


    Ctop=Con
    Cbot=1


    var andex=0
      for (var k=0; k<Con.length; k++){
        num=0
        if (Con[k]>-9999999){
          num=1}
          if (num==0){
            andex=k}
          }

      if (andex>0){
      Ctop=Con.substring(0,andex)
      Cbot=Con.substring(andex+1,Con.length)}







  var rowHTML=[]
    for (var j=0; j<5; j++){
    rowHTML.push(Row[j].innerHTML)
    }

   

 var rowNums=[]
 
    for (var j=0; j<5; j++){
    var J=rowHTML[j]
    var Jtop=J
    var Jbot=1

    var andex=0
      for (var k=0; k<J.length; k++){
        num=0
        if (J[k]>-9999999){
          num=1}
          if (num==0){
            andex=k}
          }

      if (andex>0){
      Jtop=J.substring(0,andex)
      Jbot=J.substring(andex+1,J.length)}
    
    rowNums.push([Jtop,Jbot])}


       for (var j=0; j<5; j++){
        rowNums[j][0]*=Ctop
        rowNums[j][1]*=Cbot}


  

 

    for (var j=0; j<5; j++){
      if (rowNums[j][1]==1){
        Row[j].innerHTML=rowNums[j][0]
      }
      if (rowNums[j][1]!=1){
    Row[j].innerHTML=rowNums[j][0]+"&frasl;"+rowNums[j][1]
    }
    }
  




  
    constants[i-1].value=''
    reduce()
    }



    function add(Z0,Z1){
      record_state()
start=99
end=99
    if (175<Z0 && Z0<225){start=0}
    if (250<Z0 && Z0<300){start=1}
    if (325<Z0 && Z0<375){start=2}
    if (400<Z0 && Z0<450){start=3}

    if (175<Z1 && Z1<225){end=0}
    if (250<Z1 && Z1<300){end=1}
    if (325<Z1 && Z1<375){end=2}
    if (400<Z1 && Z1<450){end=3}

    b1=document.getElementById("blue1")
    b2=document.getElementById("blue2")

    b3=document.getElementById("blue3")
    b4=document.getElementById("blue4")
    b5=document.getElementById("blue5")
    g1=document.getElementById("green1")
    g2=document.getElementById("green2")
    g3=document.getElementById("green3")
    g4=document.getElementById("green4")
    g5=document.getElementById("green5")
    r1=document.getElementById("red1")
    r2=document.getElementById("red2")
    r3=document.getElementById("red3")
    r4=document.getElementById("red4")
    r5=document.getElementById("red5")
    y1=document.getElementById("yellow1")
    y2=document.getElementById("yellow2")
    y3=document.getElementById("yellow3")
    y4=document.getElementById("yellow4")
    y5=document.getElementById("yellow5")  

    b=document.getElementById("in1")
    g=document.getElementById("in2")
    r=document.getElementById("in3")
    y=document.getElementById("in4")

    constants=[b,g,r,y]
    
    B=[b1,b2,b3,b4,b5]
    G=[g1,g2,g3,g4,g5]
    R=[r1,r2,r3,r4,r5]
    Y=[y1,y2,y3,y4,y5]
    Colors=[B,G,R,Y]
    
    AddHTML=[]
    TakeHTML=[]



Con=constants[start].value



    Ctop=Con
    Cbot=1


    var andex=0
      for (var k=0; k<Con.length; k++){
        num=0
        if (Con[k]>-9999999){
          num=1}
          if (num==0){
            andex=k}
          }

      if (andex>0){
      Ctop=Con.substring(0,andex)
      Cbot=Con.substring(andex+1,Con.length)}






   

    for (j=0; j<5; j++){
        AddHTML.push(Colors[start][j].innerHTML)
        TakeHTML.push(Colors[end][j].innerHTML)
    }

 AddNums=[]
 TakeNums=[]

    for (var j=0; j<5; j++){
    var Add=AddHTML[j]
    var Take=TakeHTML[j]



    var slash1=0
      for (var k=0; k<Add.length; k++){
        num=0
        if (Add[k]>-9999999){
          num=1}
          if (num==0){
            slash1=k}
          }

    var slash2=0
      for (var k=0; k<Take.length; k++){
        num=0
        if (Take[k]>-9999999){
          num=1}
          if (num==0){
            slash2=k}
          }






      if (slash1>0){
      AddNums.push([Add.substring(0,slash1),Add.substring(slash1+1,Add.length)])}
      if (slash1==0){
        AddNums.push([Add,1])}


      if (slash2>0){
      TakeNums.push([Take.substring(0,slash2),Take.substring(slash2+1,Take.length)])}
      if (slash2==0){
        TakeNums.push([Take,1])}

      }


       for (var j=0; j<5; j++){
        c0=Ctop
        c1=Cbot
        a0=AddNums[j][0]
        a1=AddNums[j][1]
        t0=TakeNums[j][0]
        t1=TakeNums[j][1]
        n0=c0*a0*t1+t0*c1*a1
        n1=c1*a1*t1
        if (n1==1){Colors[end][j].innerHTML=n0}
        if (n1!=1){Colors[end][j].innerHTML=n0+"&frasl;"+n1}}




    constants[start].value=''
    reduce()
    }




    function reduce(){
    


    b1=document.getElementById("blue1")
    b2=document.getElementById("blue2")
    b3=document.getElementById("blue3")
    b4=document.getElementById("blue4")
    b5=document.getElementById("blue5")
    g1=document.getElementById("green1")
    g2=document.getElementById("green2")
    g3=document.getElementById("green3")
    g4=document.getElementById("green4")
    g5=document.getElementById("green5")
    r1=document.getElementById("red1")
    r2=document.getElementById("red2")
    r3=document.getElementById("red3")
    r4=document.getElementById("red4")
    r5=document.getElementById("red5")
    y1=document.getElementById("yellow1")
    y2=document.getElementById("yellow2")
    y3=document.getElementById("yellow3")
    y4=document.getElementById("yellow4")
    y5=document.getElementById("yellow5")  

  
    
    var B=[b1,b2,b3,b4,b5]
    var G=[g1,g2,g3,g4,g5]
    var R=[r1,r2,r3,r4,r5]
    var Y=[y1,y2,y3,y4,y5]
    Colors=[B,G,R,Y]

   for (i=0; i<Colors.length; i++){
    for (j=0; j<Colors[i].length; j++){
     var n=Colors[i][j].innerHTML
  
    


   
    var slash=0
      for (var k=0; k<n.length; k++){
        num=0
        if (n[k]>-9999999){
          num=1}
          if (num==0){
            slash=k}
          }




if (slash>0){

var n0=n.substring(0,slash)
var n1=n.substring(slash+1,n.length)
lcm=1
for (m=0; m<n1+1; m++){
  if (Number.isInteger(n1/m)==true && Number.isInteger(n0/m)==true){
    lcm=m
  }
}
n0/=lcm
n1/=lcm




if (n1==1){
  Colors[i][j].innerHTML=n0}
  if (n1!=1){
  Colors[i][j].innerHTML=n0+"&frasl;"+n1}


      
    }
  }
}

    }



function clear()
{
 if (window.getSelection) {window.getSelection().removeAllRanges();}
 else if (document.selection) {document.selection.empty();}
}



function mobilize(tip) {
    tip.onmousedown = move;  

  function move(e) {
    Y0 = e.clientY;
    x0 = e.clientX;
    y0 = e.clientY;
    document.onmousemove = drag;
    document.onmouseup = close;
  }

function drag(e) {
    dx = e.clientX-x0;
    dy = e.clientY-y0;
    x0 = e.clientX;
    y0 = e.clientY;
    tip.style.left = (tip.offsetLeft + dx) + "px";
    tip.style.top = (tip.offsetTop + dy) + "px";
    clear()
  }

function close(e) {
	document.onmouseup = null;
	document.onmousemove = null;
    document.getElementById("bluetip").style.left="100px";
    document.getElementById("bluetip").style.top="0px";
    document.getElementById("greentip").style.left="100px";
    document.getElementById("greentip").style.top="75px";
    document.getElementById("redtip").style.left="100px";
    document.getElementById("redtip").style.top="150px";
    document.getElementById("yellowtip").style.left="100px";
    document.getElementById("yellowtip").style.top="225px";
    Y1=e.clientY
    add(Y0,Y1)
 }
  
  
}



bt=document.getElementById("bluetip")
mobilize(bt)

gt=document.getElementById("greentip")
mobilize(gt)
rt=document.getElementById("redtip")
mobilize(rt)

yt=document.getElementById("yellowtip")
mobilize(yt)


</script>
